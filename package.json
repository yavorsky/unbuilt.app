{
  "name": "unbuilt",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "yarn@1.22.21",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint -- --fix",
    "update-types": "turbo run update-types --filter=@unbuilt/app",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint": "^9.16.0",
    "@eslint/js": "^9.16.0",
    "prettier": "^3.4.1",
    "eslint-plugin-prettier": "^5.2.1",
    "typescript": "^5.7.2",
    "turbo": "^2.2.3"
  },
  "license": "MIT",
  "author": {
    "name": "Artem Yavorskyi",
    "email": "aqson@me.com"
  }
}
